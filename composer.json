{
    "name": "osn/invention-framework",
    "type": "library",
    "description": "The Invention Framework.",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ar Rakin",
            "email": "rakinar2@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-pdo": "*",
        "php": ">=8.0",
        "fakerphp/faker": "^1.18",
        "symfony/var-dumper": "^5.4",
        "monolog/monolog": "^2.3",
        "symfony/console": "^5.4",
        "nesbot/carbon": "^2.56",
        "psr/container": "^2.0",
        "osn/envoy": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "autoload": {
        "files": [
            "helpers/helpers.php"
        ],
        "psr-4": {
            "OSN\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OSN\\Framework\\Tests\\": "tests/"
        }
    }
}
